{
  "name": "compare-pdf",
  "version": "2.0.0",
  "packageManager": "pnpm@10.11.0",
  "type": "module",
  "description": "Standalone node module that compares pdfs",
  "main": "lib/index.cjs",
  "module": "lib/index.js",
  "source": "src/index.ts",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.js"
      },
      "require": {
        "types": "./lib/index.d.cts",
        "default": "./lib/index.cjs"
      }
    }
  },
  "scripts": {
    "prebuild": "npm run typecheck",
    "build": "tsup",
    "format": "biome format --write ./src",
    "lint": "biome lint ./src",
    "typecheck": "tsc",
    "test": "vitest",
    "test:debug": "vitest --testTimeout=30000 --no-cache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcdacz/compare-pdf.git"
  },
  "keywords": [
    "pdf",
    "graphicsMagick",
    "imageMagick",
    "compare pdf",
    "test pdf"
  ],
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/chai": "^4.3.1",
    "@types/fs-extra": "11.0.4",
    "@types/gm": "1.25.4",
    "@types/lodash": "4.17.17",
    "@types/node": "^18.0.0",
    "@types/pngjs": "6.0.5",
    "chai": "^4.2.0",
    "happy-dom": "^17.4.7",
    "jsdom": "^26.1.0",
    "tsup": "^8.5.0",
    "typescript": "5.8.3",
    "vitest": "^3.1.4"
  },
  "dependencies": {
    "canvas": "3.1.0",
    "fs-extra": "11.3.0",
    "gm": "1.25.1",
    "lodash": "4.17.21",
    "pdfjs-dist": "5.2.133",
    "pixelmatch": "7.1.0",
    "pngjs": "7.0.0"
  }
}